{
  "name": "transcriber",
  "version": "0.3.8",
  "private": true,
  "description": "UI for Transcriber using create-react-app ejected for sass",
  "keywords": [
    "transcribe",
    "react",
    "redux",
    "typescript",
    "webpack 3"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "promise": "8.0.1",
    "rc-slider": "^8.6.4",
    "react": "^16.6.3",
    "react-avatar": "^3.5.0",
    "react-avatar-editor": "^11.0.4",
    "react-bootstrap": "^0.32.4",
    "react-contextmenu": "^2.10.0",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.6.3",
    "react-dropzone": "^8.0.2",
    "react-hotkeys": "^1.1.4",
    "react-icons": "^3.2.2",
    "react-localization": "^1.0.13",
    "react-player": "^1.7.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-string-replace": "^0.4.1",
    "react-thunk": "^1.0.0",
    "react-toggle-switch": "^3.0.4",
    "react-tooltip": "^3.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "clean": "rimraf build coverage",
    "prestart": "IF NOT EXIST node_modules\\webpack-dev-server npm i -D webpack-dev-server@2.11.3",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "tslint \"src/**/*.tsx\"",
    "jest": "jest --env=jsdom"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.15",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.7.17",
    "@types/react-avatar-editor": "^10.3.2",
    "@types/react-bootstrap": "^0.32.15",
    "@types/react-dom": "^16.0.11",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-icons": "^2.2.6",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "7.1.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-sass": "^4.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "raf": "3.4.0",
    "resolve": "1.6.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
